{
  "name": "osu-beatmap-parser",
  "version": "0.1.6",
  "description": ".osu parser for nodejs",
  "main": "dist/index.js",
  "scripts": {
    "install": "prebuild-install || npm run build",
    "dev": "bun run build && node dist/verify.js",
    "build": "bun ./scripts/build.ts",
    "prebuild": "prebuild --backend cmake-js --strip --verbose",
    "prebuild:upload": "prebuild --backend cmake-js --upload",
    "cmake:help": "cmake-js help",
    "build:tsc": "tsc",
    "build:native": "cmake-js build",
    "build:native:clean": "cmake-js rebuild",
    "build:native:dev": "cmake-js build --debug",
    "configure": "cmake-js configure"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mezleca/osu-beatmap-parser.git"
  },
  "type": "commonjs",
  "dependencies": {
    "bindings": "^1.5.0",
    "prebuild-install": "^7.1.2"
  },
  "devDependencies": {
    "@types/bindings": "^1.5.5",
    "@types/node": "^24.10.1",
    "cmake-js": "^7.4.0",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^12.1.0",
    "prebuild": "^13.0.1",
    "typescript": "^5.9.3"
  }
}